[alias]
    s = status
    cv = commit -v
    branches = branch --sort=-committerdate
    glog = log --compact-summary --graph
    mine = log --all --author 'Harry White'
    sum = diff --compact-summary

[branch]
    autoSetupMerge = false

[include]
    path = ~/.gitname

[credential]
    helper = netrc -f ~/.netrc.gpg

[alias]
    s = status
    cv = commit -v
    branches = branch --sort=-committerdate
    glog = log --stat --graph
    mine = log --all --author 'Harry White'
    stat = diff --stat

[branch]
    autoSetupMerge = false

[include]
    path = ~/.gitname

[credential]
    helper = netrc -f ~/.netrc.gpg
